{
  "name": "noa-test-env",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:themes": "npm run build:theme:main && npm run build:theme:dark && npm run postcss:process",
    "build:theme:main": "sass themes/scss/smcccd-theme.scss themes/build/smcccd-theme.css --style=expanded --load-path=node_modules",
    "build:theme:dark": "sass themes/scss/smcccd-dark-theme.scss themes/build/smcccd-dark-theme.css --style=expanded --load-path=node_modules",
    "postcss:process": "postcss themes/build/*.css --replace --config postcss.config.js",
    "watch:themes": "npm run build:theme:main -- --watch",
    "start": "next start",
    "lint": "next lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "bootstrap": "^5.3.7",
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.0.0",
    "sass": "^1.89.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/eslintrc": "^3",
    "@storybook/addon-a11y": "^9.0.16",
    "@storybook/addon-docs": "^9.0.16",
    "@storybook/addon-onboarding": "^9.0.16",
    "@storybook/addon-vitest": "^9.0.16",
    "@storybook/nextjs-vite": "^9.0.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "css-loader": "^7.1.2",
    "cssnano": "^6.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-storybook": "^9.0.16",
    "mini-css-extract-plugin": "^2.9.2",
    "playwright": "^1.54.1",
    "postcss-cli": "^11.0.1",
    "sass-loader": "^16.0.5",
    "storybook": "^9.0.16",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "webpack": "^5.100.1",
    "webpack-cli": "^6.0.1"
  }
}